{
  "name": "desafio_api",
  "version": "1.0.0",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "npm install && node src/server.js",
    "predev": "npm install && npm run docker-up",
    "dev": "nodemon src/server.js",
    "docker-up": "docker-compose -f docker-compose-dev.yml up -d",
    "postdocker-up": "sleep 5 && npm run run-migration",
    "run-migration": "npx sequelize db:migrate",
    "sequelize": "sequelize",
    "docker-down": "docker-compose -f docker-compose-dev.yml down",
    "dev:debug": "nodemon --inspect src/index.js"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "nodemon": "^3.1.7",
    "prettier": "^3.4.2",
    "sequelize-cli": "^6.6.3"
  }
}
